{
    "title": "ASUHAN GIZI",
    "pages": [
     {
      "name": "page1",
      "elements": [
       {
        "type": "text",
        "name": "tgl",
        "title": "Tanggal :",
        "hideNumber": true,
        "inputType": "date"
       },
       {
        "type": "text",
        "name": "jam",
        "startWithNewLine": false,
        "title": "Jam :",
        "hideNumber": true,
        "inputType": "time"
       },
       {
        "type": "panel",
        "name": "panel1",
        "elements": [
         {
          "type": "matrixdropdown",
          "name": "antropometri",
          "title": "Antropometri",
          "titleLocation": "hidden",
          "columns": [
           {
            "name": "tinggi_lutut1",
            "title": "Tinggi Lutut",
            "cellType": "text"
           },
           {
            "name": "lla1",
            "title": "LLA",
            "cellType": "text"
           },
           {
            "name": "berat_badan",
            "title": "BB (kg)",
            "cellType": "text",
            "inputType": "number",
            "step": 1
           },
           {
            "name": "tinggi_badan",
            "title": "TB (cm)",
            "cellType": "text",
            "inputType": "number",
            "step": 1
           },
           {
            "name": "imt",
            "title": "IMT",
            "cellType": "expression",
            "expression": "pembulatan({row.berat_badan}/(({row.tinggi_badan}/100)*({row.tinggi_badan}/100)))",
            "displayStyle": "decimal"
           },
           {
            "name": "status_gizi",
            "title": "Status Gizi",
            "cellType": "expression",
            "expression": "iif({row.imt}>27.1,'Gemuk tingkat berat, IMT > 27.1',iif({row.imt}>25.1,'Gemuk tingkat ringan, IMT 25,1 - 27',iif({row.imt}>18.5,'Normal, IMT 18,5 - 25',iif({row.imt>17},'Kurus tingkat ringan, IMT 17,0 - 18,4','Kurus tingkat berat, IMT < 17'))))"
           }
          ],
          "columnLayout": "vertical",
          "choices": [
           1,
           2,
           3,
           4,
           5
          ],
          "rows": [
           {
            "value": "row",
            "text": " "
           }
          ]
         }
        ],
        "title": "Antropometri"
       },
       {
        "type": "panel",
        "name": "panel2",
        "elements": [
         {
