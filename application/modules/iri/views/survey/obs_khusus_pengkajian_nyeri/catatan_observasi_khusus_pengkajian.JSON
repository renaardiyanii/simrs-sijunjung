{
    "locale": "id",
    "title": "CATATAN OBSERVASI KHUSUS DAN PENGKAJIAN ",
    "pages": [
     {
      "name": "page1",
      "elements": [
       {
        "type": "matrixdynamic",
        "name": "question1",
        "titleLocation": "hidden",
        "columns": [
         {
          "name": "1",
          "title": "Skor Nyeri",
          "cellType": "rating",
          "rateMin": 0,
          "rateMax": 10
         },
         {
          "name": "total_skor",
          "title": "Skor :",
          "cellType": "expression",
          "expression": "{row.1}"
         },
         {
          "name": "kategori",
          "title": "Kategori :",
          "cellType": "expression",
          "expression": "iif({row.total_skor} > 7,'Berikan analgesia sesuai order (8-10)', iif({row.total_skor} < 4,'Lanjutkan observasi (0-3)','Pertimbangkan pemberian analgesik (4-7)'))"
         }
        ],
        "columnLayout": "vertical",
        "choices": [
         1,
         2,
         3,
         4,
         5
        ],
        "allowAddRows": false,
        "allowRemoveRows": false,
        "rowCount": 1
       },
       {
        "type": "matrixdynamic",
        "name": "question2",
        "titleLocation": "hidden",
        "columns": [
         {
          "name": "1",
          "title": "Skor sedasi",
          "cellType": "dropdown",
          "choices": [
           {
            "value": "0",
            "text": "sadar penuh dan dapat berbicara"
           },
           {
            "value": "1",
            "text": "ringan :( kadang kadang tertidur, mudah dibangunkan, bangun sendirnya)"
           },
           {
            "value": "2",
            "text": "sedang ( sering tertidur, mudah dibangunkan, bangun dengan rangsangan"
           },
           {
            "value": "3",
            "text": "berat (sangat mengantuk, susah dibangunkan, tertdur ssaat berbicara)"
           },
           {
            "value": "4",
            "text": "tidak sadar (tidak berespon terhadap nyeri)"
           },
           {
            "value": "x",
            "text": "tidur normal"
           }
          ]
         },
         {
          "name": "total_skor",
          "title": "Skor",
          "cellType": "expression",
          "expression": "{row.1}"
         },
         {
          "name": "intervensi",
          "title": "Intervensi",
          "cellType": "expression",
          "expression": "iif({row.total_skor} = x,'Pastikan pasien tidur dan bukan karena sedasi (x)',\niif({row.total_skor} = 2,'lanjutkan observasi atau ganti sedasinya (2)', \niif({row.total_skor} = 3 ,'pertimbangan naloxone dosis rendah  (3)',\niif({row.total_skor} = 4,'review medik segera (4)', iif({row.total_skor} < 2,'lanjutkan obserbasi atau ganti sedasinya (0-1)'))\n"
         }
        ],
        "choices": [
         1,
         2,
         3,
         4,
         5
        ],
        "allowAddRows": false,
        "allowRemoveRows": false,
        "rowCount": 1
